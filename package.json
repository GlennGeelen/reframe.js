{
  "name": "reframe.js",
  "version": "2.0.0",
  "description": "Reframe.js: responsive iframes for embedded content",
  "main": "dist/reframe.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "rollup": "rollup -c && rollup -c --environment entry:noframe",
    "rollup-jquery": "rollup -c --environment entry:jquery.noframe && rollup -c --environment entry:jquery.reframe",
    "rollup-all": "npm run rollup && npm run rollup-jquery",
    "test-reframe": "rollup && gulp test-reframe",
    "test-noframe": "rollup && gulp test-noframe",
    "test-jquery-reframe": "rollup && gulp test-jquery-reframe",
    "test-jquery-noframe": "rollup && gulp test-jquery-noframe",
    "test": "npm run rollup-all && gulp test",
    "minify": "npm run rollup-all && gulp minify",
    "build": "npm run rollup-all && gulp",
    "postpublish": "git tag $npm_package_version && git push origin --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dollarshaveclub/reframe.js.git"
  },
  "keywords": [
    "js",
    "iframes",
    "fluid",
    "responsive",
    "video",
    "fitvids",
    "youtube",
    "embeds",
    "padding-top"
  ],
  "author": "Jeff Wainwright <jjwainwright2@gmail.com> (http://jeffry.in)",
  "license": "Copyright (c) 2016 Dollar Shave Club, Inc.",
  "bugs": {
    "url": "https://github.com/dollarshaveclub/reframe.js/issues"
  },
  "homepage": "https://github.com/dollarshaveclub/reframe.js#readme",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-runtime": "^6.20.0",
    "bower": "^1.7.9",
    "debug": "^2.2.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-header": "^1.8.8",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^2.0.0",
    "jquery": "^3.1.0",
    "node-qunit-phantomjs": "^1.5.0",
    "qunit": "^1.0.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-eslint": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^1.0.1"
  }
}
